{
  "name": "spacetravelapp-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx nodemon index.js",
    "initialize": "npm i && npm run resetDB && npm run start",
    "dev": "env DELAY=1000 npx nodemon .",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.8.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodejs-nodemailer-outlook": "^1.2.3",
    "nodemailer": "^6.4.16",
    "pg": "^8.3.0",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0",
    "stripe": "^8.73.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "nodemon": "^2.0.4"
  }
